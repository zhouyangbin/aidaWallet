  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/frontend/**/*.[jt]s?(x)"
    ],
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix --ext '.ts,.tsx,.js,.jsx' -c .eslintrc.json"
  },
